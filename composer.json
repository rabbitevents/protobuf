{
    "name": "rabbitevents/protobuf",
    "description": "Protobuf extension for RabbitEvents",
    "type": "library",
    "autoload": {
        "psr-4": {
            "RabbitEvents\\Protobuf\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "rabbitevents/foundation": "^9.0",
        "google/protobuf": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6.0",
        "phpstan/phpstan": "^2.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RabbitEvents\\Protobuf\\ProtobufServiceProvider"
            ]
        }
    }
}
